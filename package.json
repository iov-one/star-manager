{
  "name": "starname-manager",
  "version": "1.3.7",
  "private": true,
  "scripts": {
    "start": "yarn generate-types && env-cmd -f .env.local react-scripts --openssl-legacy-provider start",
    "eject": "react-scripts eject",
    "build": "yarn generate-types && react-scripts build",
    "dry-build": "tsc --noEmit",
    "test": "react-scripts test --verbose --watchAll=false --testUrl='http://localhost:8080'",
    "test:watch": "react-scripts test --testUrl='http://localhost:8080'",
    "test:coverage": "react-scripts test --coverage --runInBand --watchAll=false --testUrl='http://localhost:8080'",
    "lint": "eslint --max-warnings 0 'src/**/*.ts{,x}'",
    "set-variant:local": "cp .env.local .env",
    "set-variant:devel": "cp .env.devel .env",
    "set-variant:production": "cp .env.production .env",
    "generate-project-proto-directory": "rm -rf src/proto && mkdirp src/proto",
    "generate-tx-types": "protoc --proto_path=proto --ts_proto_opt=esModuleInterop=true --ts_proto_out=src/proto $(find proto -name \"*.proto\")",
    "generate-types": "yarn generate-project-proto-directory && yarn generate-tx-types",
    "deploy:devel": "yarn set-variant:devel && env-cmd firebase deploy --project starname-manager-devel",
    "deploy:production": "yarn set-variant:production && env-cmd firebase deploy --project starname-manager-prod --message $(git rev-parse HEAD)"
  },
  "resolutions": {
    "//": "See https://github.com/facebook/create-react-app/issues/11773",
    "react-error-overlay": "6.0.9"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,ts,tsx}",
      "!src/**/*.d.ts",
      "!src/index.tsx",
      "!src/serviceWorker.ts"
    ],
    "coveragePathIgnorePatterns": [
      "./src/*/*.types.{ts,tsx}",
      "./src/index.tsx",
      "./src/serviceWorker.ts"
    ],
    "coverageReporters": [
      "json",
      "lcov",
      "text-summary",
      "clover"
    ],
    "coverageThreshold": {
      "global": {
        "statements": 95,
        "branches": 95,
        "lines": 95,
        "functions": 95
      }
    },
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ],
    "transform": {
      "^.+\\.(js|jsx|ts|tsx)$": "<rootDir>/node_modules/ts-jest"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\](?!@iov).+\\.(js|jsx|ts|tsx)$",
      "^.+\\.module\\.(css|sass|scss)$"
    ],
    "moduleNameMapper": {
      "^react-native$": "react-native-web",
      "^.+\\.module\\.(css|sass|scss)$": "identity-obj-proxy",
      "src/(.*)$/": "<rootDir>/src/$1"
    }
  },
  "browserslist": [
    ">0.2%",
    "chrome 45",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "dependencies": {
    "@cosmjs/amino": "^0.29.5",
    "@cosmjs/crypto": "^0.29.5",
    "@cosmjs/encoding": "^0.28.4",
    "@cosmjs/proto-signing": "^0.29.5",
    "@cosmjs/stargate": "^0.29.5",
    "@emotion/react": "^11.4.1",
    "@emotion/styled": "^11.3.0",
    "@fortawesome/fontawesome-free": "^5.15.1",
    "@fortawesome/fontawesome-svg-core": "^6.1.2",
    "@fortawesome/free-regular-svg-icons": "^5.10.1",
    "@fortawesome/free-solid-svg-icons": "^6.1.1",
